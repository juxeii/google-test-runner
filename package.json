{
    "name": "googletestrunner",
    "displayName": "GoogleTestRunner",
    "description": "A test runner for the C++ google test framework.",
    "version": "0.0.2",
    "icon": "images/gtest.png",
    "publisher": "nokia",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://gitlabe1.ext.net.nokia.com/reiss/googletestrunner"
    },
    "activationEvents": [
        "workspaceContains:**/CMakeLists.txt"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "googletestrunner",
            "properties": {
                "googletestrunner.buildFolder": {
                    "type": "string",
                    "default": "${workspaceFolder}/build",
                    "description": "Specifies the build folder."
                },
                "googletestrunner.gtestVerbosityLevel": {
                    "scope": "window",
                    "type": "string",
                    "description": "Controls the verbosity level of the gtest executable.",
                    "enum": [
                        "off",
                        "info",
                        "debug",
                        "trace"
                    ],
                    "default": "info"
                },
                "googletestrunner.logLevel": {
                    "scope": "window",
                    "type": "string",
                    "description": "Controls the logging verbosity.",
                    "enum": [
                        "debug",
                        "info",
                        "error"
                    ],
                    "default": "debug"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "line-number": "^0.1.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "command-exists": "^1.2.9",
        "command-exists-promise": "^2.0.2",
        "fp-ts": "^2.12.1",
        "node-sass": "^7.0.1",
        "observable-fns": "^0.6.1",
        "tree-kill": "^1.2.2",
        "triple-beam": "^1.3.0",
        "winston": "^3.7.2",
        "winston-transport": "^4.5.0"
    }
}
