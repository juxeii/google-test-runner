{
    "name": "googletestrunner",
    "displayName": "GoogleTestRunner",
    "description": "A test runner for the C++ google test framework.",
    "version": "0.0.1",
    "publisher": "nokia",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/CMakeLists.txt"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "googletestrunner",
            "properties": {
                "googletestrunner.buildFolder": {
                    "type": "string",
                    "default": "${workspaceFolder}/build",
                    "description": "Specifies the build folder."
                },
                "googletestrunner.logLevel": {
                    "scope": "window",
                    "type": "string",
                    "description": "Controls the logging verbosity.",
                    "enum": [
                        "debug",
                        "info",
                        "error"
                    ],
                    "default": "info"
                }
            }
        },
        "commands": [
            {
                "command": "googletestrunner.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "line-number": "^0.1.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "triple-beam": "^1.3.0",
        "winston": "^3.7.2",
        "winston-transport": "^4.5.0"
    }
}